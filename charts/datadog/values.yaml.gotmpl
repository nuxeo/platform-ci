nameOverride: platform
datadog:
  apiKeyExistingSecret: datadog-nxio-api
  clusterName: gke-jx-prod
  tags:
  - "environment:benchmark"
  - "staging:dev"
  - "project:jenkins-platform"
  logs:
    # by default the log collection is disabled on all container, see containerCollectAll
    enabled: true
  dogstatsd:
    useHostPort: true
agents:
  localService:
    forceLocalServiceEnabled: true
  nodeSelector:
    team: platform
    usage: benchmark
  tolerations:
  - key: team
    operator: Equal
    value: platform
    effect: NoSchedule
  - key: usage
    operator: Equal
    value: benchmark
    effect: NoSchedule
